{
    "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
    "author": "Jan Reinsch <jan@coded-aesthetics.com>",
    "name": "Wallust",
    "themes": [
        {
            "name": "Wallust",
            "appearance": "dark",
            "style": {
                // Basic text and background colors
                "background": "#131318",
                "text": "#FDFAE5",
                "text.muted": "#CAC8B7",
                "text.placeholder": "#B1AFA0",
                "text.disabled": "#989689",
                "text.accent": "#FC9192",

                // Icon colors - match text hierarchy
                "icon": "#FDFAE5",
                "icon.muted": "#CAC8B7",
                "icon.disabled": "#989689",
                "icon.placeholder": "#B1AFA0",
                "icon.accent": "#FC9192",

                // Border colors - subtle but visible
                "border": "#65645C",
                "border.variant": "#4C4B45",
                "border.focused": "#FC9192",
                "border.selected": "#FC9192",
                "border.transparent": "#00000000",
                "border.disabled": "#33322E",

                // Surface backgrounds - slightly elevated from main background
                "surface.background": "#131318",
                "elevated_surface.background": "#2B2B2F",

                // Interactive element states
                "element.background": "transparent",
                "element.hover": "#363633",
                "element.active": "#523133",
                "element.selected": "#44292A",
                "element.disabled": "#1F1F24",

                // Ghost elements - more subtle interactions
                "ghost_element.background": "transparent",
                "ghost_element.hover": "#1B1B19",
                "ghost_element.active": "#363633",
                "ghost_element.selected": "#29191A",
                "ghost_element.disabled": "transparent",

                // Drag and drop target
                "drop_target.background": "#5F393C",

                // Application frame elements
                "status_bar.background": "#1F1F24",
                "title_bar.background": "#131318",
                "title_bar.inactive_background": "#1A1A1F",
                "toolbar.background": "#131318",

                // Side panels
                "panel.background": "#131318",
                "panel.focused_border": "#FC9192",

                // Editor panes
                "pane.focused_border": "#FC9192",
                "pane_group.border": "#65645C",

                // Tab bar and tabs
                "tab_bar.background": "#1F1F24",
                "tab.inactive_background": "transparent",
                "tab.active_background": "#131318",

                // Main editor area
                "editor.background": "#131318",
                "editor.foreground": "#FDFAE5",
                "editor.gutter.background": "#131318",
                "editor.subheader.background": "#1F1F24",
                "editor.active_line.background": "#1B1B19",
                "editor.highlighted_line.background": "#371D2E",
                "editor.line_number": "#989689",
                "editor.active_line_number": "#FDFAE5",
                "editor.invisible": "#7F7D73",
                "editor.wrap_guide": "#65645C",
                "editor.active_wrap_guide": "#989689",
                "editor.indent_guide": "#65645C",
                "editor.indent_guide_active": "#989689",
                "editor.document_highlight.read_background": "#44243A",
                "editor.document_highlight.write_background": "#2F2B34",
                "editor.document_highlight.bracket_background": "#44292A",

                // Integrated terminal
                "terminal.background": "#131318",
                "terminal.foreground": "#FDFAE5",
                "terminal.bright_foreground": "#FDFBE8",
                "terminal.dim_foreground": "#CAC8B7",
                "terminal.ansi.background": "#131318",
                "terminal.ansi.black": "#3A3A40",
                "terminal.ansi.red": "#AA98B6",
                "terminal.ansi.green": "#7A9FFE",
                "terminal.ansi.yellow": "#FE7FCF",
                "terminal.ansi.blue": "#FC9192",
                "terminal.ansi.magenta": "#FAC49F",
                "terminal.ansi.cyan": "#F9F0B4",
                "terminal.ansi.white": "#F4EFD1",
                "terminal.ansi.bright_black": "#AAA792",
                "terminal.ansi.bright_red": "#AA98B6",
                "terminal.ansi.bright_green": "#7A9FFE",
                "terminal.ansi.bright_yellow": "#FE7FCF",
                "terminal.ansi.bright_blue": "#FC9192",
                "terminal.ansi.bright_magenta": "#FAC49F",
                "terminal.ansi.bright_cyan": "#F9F0B4",
                "terminal.ansi.bright_white": "#F4EFD1",

                // Search functionality
                "search.match_background": "#6E3A5D",

                // Scrollbar elements
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "transparent",
                "scrollbar.thumb.background": "#7F7D73",
                "scrollbar.thumb.border": "transparent",
                "scrollbar.thumb.hover_background": "#B1AFA0",

                // Status indicators - success, warning, error, info
                "success": "#7A9FFE",
                "success.background": "#232C46",
                "success.border": "#7A9FFE",

                "warning": "#FE7FCF",
                "warning.background": "#44243A",
                "warning.border": "#FE7FCF",

                "error": "#AA98B6",
                "error.background": "#2F2B34",
                "error.border": "#AA98B6",

                "info": "#FC9192",
                "info.background": "#44292A",
                "info.border": "#FC9192",

                "hint": "#CAC8B7",
                "hint.background": "#292926",
                "hint.border": "#CAC8B7",

                // Git status colors
                "created": "#7A9FFE",
                "created.background": "#232C46",
                "created.border": "#7A9FFE",

                "modified": "#FE7FCF",
                "modified.background": "#44243A",
                "modified.border": "#FE7FCF",

                "deleted": "#AA98B6",
                "deleted.background": "#2F2B34",
                "deleted.border": "#AA98B6",

                "renamed": "#FC9192",
                "renamed.background": "#44292A",
                "renamed.border": "#FC9192",

                "conflict": "#FAC49F",
                "conflict.background": "#44362E",
                "conflict.border": "#FAC49F",

                "ignored": "#B1AFA0",
                "ignored.background": "#1B1B19",
                "ignored.border": "#B1AFA0",

                "hidden": "#989689",
                "hidden.background": "transparent",
                "hidden.border": "#989689",

                "predictive": "#B1AFA0",
                "predictive.background": "#1B1B19",
                "predictive.border": "#B1AFA0",

                "unreachable": "#7F7D73",
                "unreachable.background": "#1A1A1F",
                "unreachable.border": "#7F7D73",

                // Link styling
                "link_text.hover": "#FC9C9D",

                // Syntax highlighting - follows standard color assignments
                "syntax": {
                    // Comments - muted but readable
                    "comment": {
                        "color": "#CAC8B7",
                        "font_style": "italic"
                    },
                    "comment.doc": {
                        "color": "#E4E1CE",
                        "font_style": "italic"
                    },

                    // Keywords
                    "keyword": {
                        "color": "#AA98B6"
                    },

                    // Strings
                    "string": {
                        "color": "#7A9FFE"
                    },
                    "string.escape": {
                        "color": "#F9F0B4"
                    },
                    "string.regex": {
                        "color": "#F9F0B4"
                    },

                    // Numbers and constants
                    "number": {
                        "color": "#FE7FCF"
                    },
                    "constant": {
                        "color": "#FE7FCF"
                    },
                    "boolean": {
                        "color": "#FAC49F"
                    },

                    // Functions
                    "function": {
                        "color": "#FC9192"
                    },
                    "constructor": {
                        "color": "#FC9192"
                    },

                    // Types
                    "type": {
                        "color": "#FAC49F"
                    },
                    "enum": {
                        "color": "#FAC49F"
                    },

                    // Variables
                    "variable": {
                        "color": "#FDFAE5"
                    },
                    "variable.special": {
                        "color": "#FAC49F"
                    },

                    // Properties and attributes
                    "property": {
                        "color": "#FC9192"
                    },
                    "attribute": {
                        "color": "#FE7FCF"
                    },

                    // Operators - typically cyan (color6)
                    "operator": {
                        "color": "#F9F0B4"
                    },

                    // Punctuation - subtle but visible
                    "punctuation": {
                        "color": "#E4E1CE"
                    },
                    "punctuation.bracket": {
                        "color": "#FDFAE5"
                    },
                    "punctuation.delimiter": {
                        "color": "#E4E1CE"
                    },

                    // Tags (HTML/XML)
                    "tag": {
                        "color": "#AA98B6"
                    },

                    // Labels and identifiers
                    "label": {
                        "color": "#FC9192"
                    },

                    // Text literals
                    "text.literal": {
                        "color": "#7A9FFE"
                    },

                    // Titles and headings
                    "title": {
                        "color": "#FC9192",
                        "font_weight": 700
                    },

                    // Links in markdown/docs
                    "link_text": {
                        "color": "#FC9192",
                        "font_style": "italic"
                    },
                    "link_uri": {
                        "color": "#F9F0B4"
                    },

                    // Text emphasis
                    "emphasis": {
                        "font_style": "italic"
                    },
                    "emphasis.strong": {
                        "font_weight": 700
                    },

                    // Special syntax elements
                    "embedded": {
                        "color": "#FDFAE5"
                    },
                    "preproc": {
                        "color": "#AA98B6"
                    },
                    "primary": {
                        "color": "#FDFAE5"
                    },
                    "variant": {
                        "color": "#FAC49F"
                    },

                    // Editor hints and diagnostics
                    "hint": {
                        "color": "#CAC8B7",
                        "font_weight": 700
                    },
                    "predictive": {
                        "color": "#B1AFA0",
                        "font_style": "italic"
                    }
                },

                // Collaborative editing - different users get different colors
                "players": [
                    {
                        "cursor": "#FC9192",
                        "background": "#FC9192",
                        "selection": "#5F393C"
                    },
                    {
                        "cursor": "#7A9FFE",
                        "background": "#7A9FFE",
                        "selection": "#313E61"
                    },
                    {
                        "cursor": "#FE7FCF",
                        "background": "#FE7FCF",
                        "selection": "#603351"
                    },
                    {
                        "cursor": "#FAC49F",
                        "background": "#FAC49F",
                        "selection": "#5E4C40"
                    }
                ]
            }
        }
    ]
}
